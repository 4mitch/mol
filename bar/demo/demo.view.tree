$mol_bar_demo_search $mol_row
	sub /
		<= Subscription $mol_bar
			sub /
				<= String $mol_string
					value?val <=> value?val \
				<= Submit $mol_button
					sub /
						\submit

$mol_bar_demo_login $mol_row
	sub /
		<= Subscription $mol_bar
			sub /
				<= String $mol_string
					hint \E-mail
					value?val <=> value?val \
				<= Confirm $mol_check_box
					label / \Confirm
				<= Submit $mol_button
					sub /
						\submit
