$mol_rower_demo $mol_rower
	heightMinimal 50
	childs /
		< titler $mol_stringer
			hint \Title
			value > title \
		< publisher $mol_checker
			childs / \Shared
			checked > publish false
		< buttonDrop $mol_clicker_minor
			childs / \Drop
			eventClick > eventLog null
		< portioner $mol_portioner
			portion 0.5
