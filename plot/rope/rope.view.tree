$mol_plot_rope $mol_plot_graph
	sub /
		<= Line $mol_plot_line
			points_scaled <= points_scaled -
			color <= color -
			type <= type \solid
			Sample => Line_sample
		<= Dots $mol_plot_dot
			points_scaled <= points_scaled -
			color <= color -
			Sample => Dots_sample
	Sample $mol_plot_graph_sample
		sub /
			<= Line_sample -
			<= Dots_sample -
