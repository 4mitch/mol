$mol_stacker $mol_viewer childs /
	< mainer $mol_stacker_panel
		focused > mainerFocused : true
		childs / < main : null
	< addoner $mol_stacker_panel
		focused > addonerFocused : false
		childs / < addon : null

$mol_stacker_panel $mol_viewer
	focuses : null
	attr * mol_stacker_focused < focused : false
	event *
		click > focuses
		touchmove > focuses
		scroll > focuses
		wheel > focuses
