$mol_app_studio $mol_book
	value_overrided!id?val null
	-
	pages /
		<= Preview_page $mol_page
			title <= preview_title @ \Preview
			tools <= tools_main /
			body /
				<= Selector $mol_app_studio_selector
					Sub <= Block $mol_view
					path?val <=> path?val /
			minimal_width 500
		<= Editor_page $mol_page
			title <= editor_title @ \Properties
			event_top?val <=> event_front_up?val -
			tools /
				<= Close $mol_link
					sub /
						<= Close_icon $mol_icon_cross
					arg <= close_arg *
						path null
			Body <= Editor_body $mol_scroll
				sub <= fields /
			minimal_width 500
	Placeholder null
	-
	Prop!id $mol_row
		sub /
			<= Prop_dive!id $mol_link
				title <= prop_title!id \
				arg <= prop_arg!id *
					path <= prop_path!id \
			<= Prop_type!id $mol_select
				value?val <=> prop_type!id?val \null
				clearable false
				searchable false
				dictionary <= type_options *
					any \Any
					boolean \Bool
					number \Numb
					string \Str
					list \List
					dict \Dict
					object \Obj
					get \<=
					bind \<=>
			<= prop_controls!id /
				<= Boolean_field!id $mol_switch
					value?val <=> value_effective!id?val null
					options *
						true \True
						false \False
				<= Number_field!id $mol_number
					value?val <=> value_effective!id?val null
					hint <= number_hint!id \
				<= String_field!id $mol_string
					value?val <=> value_effective!id?val null
					hint <= string_hint!id \
				<= Element_field!id $mol_select
					value?val <=> value_effective!id?val null
					options <= view_options /
				<= Bind_field!id $mol_select
					value?val <=> value_effective!id?val null
					options <= bind_options /
				<= List_field!id $mol_select
					value?val <=> value_effective!id?val null
					options <= view_options /
	-
	block?val <= block_default \mol_view
	registry $mol_tree

$mol_app_studio_selector $mol_ghost
	event *
		contextmenu?event <=> select?event null
		dblclick?event <=> select?event null
	path?val /
