$mol_app_supplies_root $mol_book
	-
	enter $mol_app_supplies_enter
		minimal_width 300
		entered?val <=> entered?val false
	-
	lister $mol_app_supplies_list
		minimal_width 300
		supplies <= supplies /
		search_query?val <=> search_query?val \
	-
	detailer $mol_app_supplies_detail
		minimal_width 400
		supply <= supply null
