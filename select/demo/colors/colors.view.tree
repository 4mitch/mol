$mol_select_demo_colors $mol_labeler
	title \Choose some color
	Content $mol_select
		dictionary <= colors *
		option_label!id <= color_name!id \
		option_content!id <= option_content!id /
			<= Color_option_row!id $mol_row
				sub /
					<= Color_option!id $mol_select_colors_preview
						color <= color!id \
					<= color_name!id \

$mol_select_colors_preview $mol_view
	style *
		background <= color \
